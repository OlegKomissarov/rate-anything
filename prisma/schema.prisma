generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model rates {
  id        Int    @id @default(autoincrement())
  subject   String @db.VarChar(255)
  rate      Int
  username  String @db.VarChar(255)
  useremail String @db.VarChar(255)
}
